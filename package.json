{
  "name": "ember-uikit",
  "version": "9.1.2",
  "description": "The ember implementation of UIkit",
  "keywords": [
    "ember-addon",
    "uikit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adfinis/ember-uikit"
  },
  "license": "MIT",
  "author": "Adfinis AG <info@adfinis.com>",
  "directories": {
    "test": "tests"
  },
  "homepage": "https://adfinis.github.io/ember-uikit/",
  "bugs": {
    "url": "https://github.com/adfinis/ember-uikit/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "ember-template-lint --fix"
    ],
    "*.hbs": "ember-template-lint --fix",
    "*.scss": "stylelint --fix",
    "*.{json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@babel/core": "^7.24.9",
    "@ember/string": "^4.0.0",
    "@embroider/util": "^1.13.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.7.4",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-focus-trap": "^1.1.0",
    "ember-modifier": "^4.2.0",
    "ember-toggle": "^9.0.3",
    "ember-truth-helpers": "^4.0.3",
    "uikit": "^3.21.8"
  },
  "devDependencies": {
    "@adfinis/eslint-config": "2.1.1",
    "@adfinis/semantic-release-config": "5.0.0",
    "@babel/eslint-parser": "7.25.1",
    "@babel/plugin-proposal-decorators": "7.24.7",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@ember/optional-features": "2.1.0",
    "@ember/test-helpers": "3.3.1",
    "@embroider/test-setup": "4.0.0",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "8.2.2",
    "ember-cli": "5.10.0",
    "ember-cli-app-version": "7.0.0",
    "ember-cli-clean-css": "3.0.0",
    "ember-cli-code-coverage": "3.0.0",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-code-snippet": "3.0.0",
    "ember-concurrency": "4.0.2",
    "ember-load-initializers": "2.1.2",
    "ember-prism": "1.0.0",
    "ember-qunit": "8.1.0",
    "ember-resolver": "12.0.1",
    "ember-source": "5.10.1",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "6.0.0",
    "ember-template-lint-plugin-prettier": "5.0.0",
    "ember-try": "3.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-ember": "12.1.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.10.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-qunit": "8.1.1",
    "husky": "9.1.2",
    "lint-staged": "15.2.7",
    "loader.js": "4.7.0",
    "prettier": "3.3.3",
    "qunit": "2.21.1",
    "qunit-dom": "3.2.0",
    "sass": "1.77.8",
    "semantic-release": "24.0.0",
    "stylelint": "16.7.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-prettier": "5.0.2",
    "stylelint-scss": "6.4.1",
    "webpack": "5.93.0"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adfinis.github.io/ember-uikit"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "release": {
    "extends": "@adfinis/semantic-release-config"
  }
}
