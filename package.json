{
  "name": "ember-uikit",
  "version": "6.1.1",
  "description": "The ember implementation of UIkit",
  "keywords": [
    "ember-addon",
    "uikit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adfinis/ember-uikit"
  },
  "license": "MIT",
  "author": "Adfinis AG <info@adfinis.com>",
  "directories": {
    "test": "tests"
  },
  "homepage": "https://adfinis.github.io/ember-uikit/",
  "bugs": {
    "url": "https://github.com/adfinis/ember-uikit/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": ["yarn lint:js:fix", "yarn lint:hbs:fix"],
    "*.hbs": "yarn lint:hbs:fix",
    "*.{scss,json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@ember/string": "^3.0.1",
    "@embroider/util": "^1.10.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.6.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-focus-trap": "^1.0.1",
    "ember-modifier": "^4.1.0",
    "ember-toggle": "^9.0.3",
    "uikit": "^3.16.13"
  },
  "devDependencies": {
    "@adfinis-sygroup/eslint-config": "1.6.1",
    "@adfinis-sygroup/semantic-release-config": "3.4.0",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.9.3",
    "@embroider/test-setup": "2.1.1",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "7.6.0",
    "ember-cli": "4.11.0",
    "ember-cli-app-version": "6.0.0",
    "ember-cli-code-coverage": "2.0.0",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-deploy": "2.0.0",
    "ember-cli-deploy-build": "2.0.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-code-snippet": "3.0.0",
    "ember-composable-helpers": "5.0.0",
    "ember-concurrency": "2.3.7",
    "ember-load-initializers": "2.1.2",
    "ember-prism": "0.13.0",
    "ember-qunit": "6.2.0",
    "ember-resolver": "10.0.0",
    "ember-source": "4.11.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.7.1",
    "ember-template-lint-plugin-prettier": "4.1.0",
    "ember-truth-helpers": "3.1.1",
    "ember-try": "2.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-ember": "11.4.8",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.4",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "loader.js": "4.7.0",
    "prettier": "2.8.7",
    "qunit": "2.19.4",
    "qunit-dom": "2.0.0",
    "sass": "1.60.0",
    "webpack": "5.76.3"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://docs.adfinis.com/ember-uikit"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  }
}
