{
  "name": "ember-uikit",
  "version": "9.2.1",
  "description": "The ember implementation of UIkit",
  "keywords": [
    "ember-addon",
    "uikit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adfinis/ember-uikit"
  },
  "license": "MIT",
  "author": "Adfinis AG <info@adfinis.com>",
  "directories": {
    "test": "tests"
  },
  "homepage": "https://adfinis.github.io/ember-uikit/",
  "bugs": {
    "url": "https://github.com/adfinis/ember-uikit/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "ember-template-lint --fix"
    ],
    "*.hbs": "ember-template-lint --fix",
    "*.scss": "stylelint --fix",
    "*.{json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@ember/string": "^4.0.0",
    "@embroider/util": "^1.13.1",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-focus-trap": "^1.1.0",
    "ember-modifier": "^4.2.0",
    "ember-template-imports": "^4.3.0",
    "ember-toggle": "^9.0.3",
    "ember-truth-helpers": "^4.0.3 || ^5.0.0",
    "uikit": "^3.23.0"
  },
  "devDependencies": {
    "@adfinis/eslint-config": "3.0.2",
    "@adfinis/semantic-release-config": "5.0.0",
    "@babel/eslint-parser": "7.28.5",
    "@babel/plugin-proposal-decorators": "7.28.0",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@ember/optional-features": "2.2.0",
    "@ember/test-helpers": "5.4.0",
    "@embroider/macros": "1.19.2",
    "@embroider/test-setup": "4.0.0",
    "@eslint/js": "9.38.0",
    "@glimmer/component": "2.0.0",
    "@glimmer/tracking": "1.1.2",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "9.2.1",
    "ember-cli": "6.8.0",
    "ember-cli-app-version": "7.0.0",
    "ember-cli-clean-css": "3.0.0",
    "ember-cli-code-coverage": "3.1.0",
    "ember-cli-dependency-checker": "3.3.3",
    "ember-cli-deprecation-workflow": "3.4.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-code-snippet": "3.0.0",
    "ember-concurrency": "5.1.0",
    "ember-load-initializers": "3.0.1",
    "ember-qunit": "9.0.4",
    "ember-resolver": "13.1.1",
    "ember-shiki": "0.3.0",
    "ember-source": "6.8.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "7.9.3",
    "ember-template-lint-plugin-prettier": "5.0.0",
    "ember-try": "4.0.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-ember": "12.7.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-qunit": "8.2.5",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "loader.js": "4.7.0",
    "prettier": "3.6.2",
    "prettier-plugin-ember-template-tag": "2.1.0",
    "qunit": "2.24.2",
    "qunit-dom": "3.5.0",
    "sass": "1.93.2",
    "semantic-release": "25.0.1",
    "stylelint": "16.25.0",
    "stylelint-config-standard-scss": "16.0.0",
    "stylelint-prettier": "5.0.3",
    "stylelint-scss": "6.12.1",
    "webpack": "5.102.1"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">= 20.11"
  },
  "ember": {
    "edition": "octane"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adfinis.github.io/ember-uikit"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "release": {
    "extends": "@adfinis/semantic-release-config"
  }
}
