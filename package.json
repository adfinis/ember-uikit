{
  "name": "ember-uikit",
  "version": "9.0.0",
  "description": "The ember implementation of UIkit",
  "keywords": [
    "ember-addon",
    "uikit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adfinis/ember-uikit"
  },
  "license": "MIT",
  "author": "Adfinis AG <info@adfinis.com>",
  "directories": {
    "test": "tests"
  },
  "homepage": "https://adfinis.github.io/ember-uikit/",
  "bugs": {
    "url": "https://github.com/adfinis/ember-uikit/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "ember-template-lint --fix"
    ],
    "*.hbs": "ember-template-lint --fix",
    "*.scss": "stylelint --fix",
    "*.{json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@ember/string": "^3.1.1",
    "@embroider/util": "^1.12.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.7.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-focus-trap": "^1.1.0",
    "ember-modifier": "^4.1.0",
    "ember-toggle": "^9.0.3",
    "ember-truth-helpers": "^4.0.3",
    "uikit": "^3.17.11"
  },
  "devDependencies": {
    "@adfinis/eslint-config": "2.1.0",
    "@adfinis/semantic-release-config": "4.1.0",
    "@babel/eslint-parser": "7.23.10",
    "@babel/plugin-proposal-decorators": "7.24.0",
    "@ember/optional-features": "2.1.0",
    "@ember/test-helpers": "3.3.0",
    "@embroider/compat": "3.4.5",
    "@embroider/core": "3.4.5",
    "@embroider/test-setup": "3.0.3",
    "@embroider/webpack": "3.2.2",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "8.2.2",
    "ember-cli": "5.6.0",
    "ember-cli-app-version": "6.0.1",
    "ember-cli-clean-css": "3.0.0",
    "ember-cli-code-coverage": "2.1.1",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-code-snippet": "3.0.0",
    "ember-concurrency": "3.1.1",
    "ember-load-initializers": "2.1.2",
    "ember-prism": "0.13.0",
    "ember-qunit": "8.0.2",
    "ember-resolver": "11.0.1",
    "ember-source": "5.3.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.13.0",
    "ember-template-lint-plugin-prettier": "5.0.0",
    "ember-try": "3.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-ember": "12.0.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-qunit": "8.1.1",
    "husky": "8.0.3",
    "lint-staged": "15.2.2",
    "loader.js": "4.7.0",
    "prettier": "3.2.5",
    "qunit": "2.20.1",
    "qunit-dom": "3.0.0",
    "sass": "1.71.1",
    "semantic-release": "22.0.10",
    "stylelint": "16.2.1",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-prettier": "5.0.0",
    "stylelint-scss": "6.1.0",
    "webpack": "5.90.3"
  },
  "packageManager": "pnpm@8.8.0",
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://adfinis.github.io/ember-uikit"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "release": {
    "extends": "@adfinis/semantic-release-config"
  }
}
