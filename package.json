{
  "name": "ember-uikit",
  "version": "6.1.1",
  "description": "The ember implementation of UIkit",
  "keywords": [
    "ember-addon",
    "uikit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adfinis/ember-uikit"
  },
  "license": "MIT",
  "author": "Adfinis AG <info@adfinis.com>",
  "directories": {
    "test": "tests"
  },
  "homepage": "https://adfinis.github.io/ember-uikit/",
  "bugs": {
    "url": "https://github.com/adfinis/ember-uikit/issues"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "npm-run-all --print-name \"lint\" \"test:*\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ember/string": "^3.0.0",
    "@embroider/util": "^1.9.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.4.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-focus-trap": "^1.0.1",
    "ember-modifier": "^3.2.7",
    "ember-toggle": "^9.0.3",
    "uikit": "^3.15.11"
  },
  "devDependencies": {
    "@adfinis-sygroup/eslint-config": "1.5.0",
    "@adfinis-sygroup/semantic-release-config": "3.4.0",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.8.1",
    "@embroider/test-setup": "1.8.3",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "4.8.0",
    "ember-cli-app-version": "5.0.0",
    "ember-cli-code-coverage": "1.0.3",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-deploy": "1.0.2",
    "ember-cli-deploy-build": "2.0.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-code-snippet": "3.0.0",
    "ember-composable-helpers": "5.0.0",
    "ember-concurrency": "2.3.7",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.2",
    "ember-prism": "0.13.0",
    "ember-qunit": "6.0.0",
    "ember-resolver": "8.0.3",
    "ember-source": "4.8.1",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "4.17.0",
    "ember-template-lint-plugin-prettier": "4.0.0",
    "ember-truth-helpers": "3.1.1",
    "ember-try": "2.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "11.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "qunit": "2.19.3",
    "qunit-dom": "2.0.0",
    "sass": "1.56.2",
    "webpack": "5.74.0"
  },
  "ember": {
    "edition": "octane"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://docs.adfinis.com/ember-uikit"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  }
}
