<div class="uk-container uk-padding">
  <div uk-grid class="uk-grid-divider">
    <div class="uk-width-1-1 uk-width-1-4@m">
      <ul class="uk-nav uk-nav-primary">
        <li>{{#link-to "index"}}Introduction{{/link-to}}</li>
        <li class="uk-nav-divider"></li>
        <li class="uk-parent">
          <a href="" class="uk-disabled">Components</a>
          <ul class="uk-nav-sub">
            <li>{{#link-to "docs.components.badge"}}Badge{{/link-to}}</li>
            <li>{{#link-to "docs.components.button"}}Button{{/link-to}}</li>
            <li>{{#link-to "docs.components.card"}}Card{{/link-to}}</li>
            <li>{{#link-to "docs.components.icon"}}Icon{{/link-to}}</li>
            <li>{{#link-to "docs.components.label"}}Label{{/link-to}}</li>
            <li>{{#link-to "docs.components.list"}}List{{/link-to}}</li>
            <li>{{#link-to "docs.components.modal"}}Modal{{/link-to}}</li>
            <li>{{#link-to "docs.components.sortable"}}Sortable{{/link-to}}</li>
            <li>{{#link-to "docs.components.spinner"}}Spinner{{/link-to}}</li>
            <li>{{#link-to "docs.components.subnav"}}Subnav{{/link-to}}</li>
            <li>{{#link-to "docs.components.switcher"}}Switcher{{/link-to}}</li>
            <li>{{#link-to "docs.components.tab"}}Tab{{/link-to}}</li>
            <li>{{#link-to "docs.components.toggle-switch"}}Toggle Switch{{/link-to}}</li>
          </ul>
        </li>
        <li class="uk-nav-divider"></li>
        <li class="uk-parent">
          <a href="" class="uk-disabled">Utilities</a>
          <ul class="uk-nav-sub">
            <li>{{#link-to "docs.utilities.flex"}}Flex{{/link-to}}</li>
            <li>{{#link-to "docs.utilities.width"}}Width{{/link-to}}</li>
          </ul>
        </li>
        <li class="uk-nav-divider"></li>
        <li class="uk-parent">
          <a href="" class="uk-disabled">Services</a>
          <ul class="uk-nav-sub">
            <li>{{#link-to "docs.services.notification"}}Notification{{/link-to}}</li>
          </ul>
        </li>
      </ul>
    </div>
    <div class="uk-width-1-1 uk-width-3-4@m">
      {{outlet}}
    </div>
  </div>
  <div class="uk-text-center uk-margin-xlarge-top">
    <p>
      ember-uikit v{{app-version hideSha=true}}
    </p>
    <div uk-grid class="uk-flex uk-flex-center">
      <div class="uk-width-1-3 uk-width-1-6@m">
        <a href="https://github.com/adfinis-sygroup/ember-uikit" class="uk-link-text">
          {{uk-icon "github"}}<br>Github
        </a>
      </div>
      <div class="uk-width-1-3 uk-width-1-6@m">
        <a
          href="https://github.com/adfinis-sygroup/ember-uikit/issues"
          class="uk-link-text"
        >
          {{uk-icon "commenting"}}<br>Issues
        </a>
      </div>
      <div class="uk-width-1-3 uk-width-1-6@m">
        <a
          href="https://github.com/adfinis-sygroup/ember-uikit/blob/master/CHANGELOG.md"
          class="uk-link-text"
        >
          {{uk-icon "history"}}<br>Changelog
        </a>
      </div>
    </div>
